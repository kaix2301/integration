pipeline {
    agent any
    stages {
        stage('资源获取') {
            steps {
                echo '资源获取中...'
                sleep 30 // 假装在获取资源，维持30秒
            }
        }
        stage('资源解压') {
            steps {
                echo '资源解压中...'
                sleep 30 // 假装在解压资源，维持30秒
            }
        }
        stage('代码编译') {
            steps {
                echo '代码编译中...'
                sleep 30 // 假装在编译代码，维持30秒
            }
        }
        stage('资源打包') {
            steps {
                echo '资源打包中...'
                sleep 30 // 假装在打包资源，维持30秒
            }
        }
        stage('资源发布') {
            steps {
                echo '资源发布中...'
                sleep 30 // 假装在发布资源，维持30秒
            }
        }
    }
}
